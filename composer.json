    "keywords":["yandex","dns"],
    "type":"library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "guzzle/http": "3.8.1",
        "guzzle/service": "3.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Yandex": "src/",
            "Yandex\\Tests": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alexandr Medvedev",
            "email": "alexandr.mdr@gmail.com"
        }
